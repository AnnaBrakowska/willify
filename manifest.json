{
  "name": "Willifier",
  "version": "1.0",
  "description": "Willifies NY Times Page",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "Willify_Logo.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.nytimes.com/"],
      "js": ["script.js"]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "web_accessible_resources": ["*.wav", "*.png"]
}
